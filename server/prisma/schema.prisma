generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Note {
  id        Int      @id @default(autoincrement())
  title     String   @unique @db.VarChar(255)
  body      String   @db.Text
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
